(defun replace-amp ()
  "Postprocess HTML file to unescape the ampersand."
  (interactive)
  (while (re-search-forward "&lt;td&gt;&amp;amp;\\([^&lt;;]+;\\)" nil t)
    (replace-match (concat "&lt;td&gt;&amp;" (match-string 1)) t t)))
