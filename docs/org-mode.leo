(defun org-publish-cache-ctime-of-src (filename)
  "Get the files ctime as integer."
  (let ((src-attr (file-attributes filename)))
    (+
     (lsh (car (nth 5 src-attr)) 16)
     (cadr (nth 5 src-attr)))))
