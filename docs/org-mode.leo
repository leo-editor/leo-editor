(defun org-export-latex-protect-amp (s)
  (while (string-match "\\([^\\\\]\\)\\(&amp;\\)" s)
    (setq s (replace-match (concat (match-string 1 s) "\\" (match-string 2 s))
			   t t s)))
  s)
