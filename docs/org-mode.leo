	(regexp (concat "\\&lt;\\(" org-deadline-string
			"\\|" org-scheduled-string
			"\\) *&lt;\\([^&gt;]+\\)&gt;"))
	(callback
	 (lambda () (not
		     (time-less-p
		      (org-time-string-to-time (match-string 2))
		      (org-time-string-to-time date))))))
