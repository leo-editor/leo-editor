<t tx="ekr.20111006212449.11851">def __init__(self, c):
    self.c = c
    self.errors = 0
    # Commanders...
    self.core = None  # leoPy.leo.
    self.settings = None  # leoSettings.leo.
    self.user_settings = None  # myLeoSettings.leo
