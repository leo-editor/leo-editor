<t tx="ekr.20031218072017.2369">def skip_string(s: str, i: int) -&gt; int:
    """Scan forward to the end of a string."""
    delim = s[i]
    i += 1
    assert delim in '\'"', (repr(delim), repr(s))
    n = len(s)
    while i &lt; n and s[i] != delim:
        if s[i] == '\\':
            i += 2
        else:
            i += 1
    if i &gt;= n:
        pass
    elif s[i] == delim:
        i += 1
    return i
