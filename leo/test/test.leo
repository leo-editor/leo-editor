	var winHeight = findWindowHeight();
	var winBot = winTop + winHeight;
	if(posTop &lt; winTop) {
		return posTop;
	} else if(posBot &gt; winBot) {
		if(e.offsetHeight &lt; winHeight)
			return posTop - (winHeight - e.offsetHeight);
