<t tx="ekr.20111017204736.15898">def getDocString(s: str) -&gt; str:
    """Return the text of the first docstring found in s."""
    tags = ('"""', "'''")
    tag1, tag2 = tags
    i1, i2 = s.find(tag1), s.find(tag2)
    if i1 == -1 and i2 == -1:
        return ''
    if i1 &gt; -1 and i2 &gt; -1:
        i = min(i1, i2)
    else:
        i = max(i1, i2)
    tag = s[i : i + 3]
    assert tag in tags
    j = s.find(tag, i + 3)
    if j &gt; -1:
        return s[i + 3 : j]
    return ''
