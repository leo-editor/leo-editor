        page += 1
        if page &gt; limit:
            g.trace('too many pages')
            break
    state = state.capitalize()
    if self.milestone:
        root.h = f"{total} {state} {label} issues for milestone {self.milestone}"
    else:
        root.h = f"{total} {state} {label} issues"
