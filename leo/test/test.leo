def getFileName ():

    '''Return the filename.

    This is a hack for when Leo is started from a script that also starts IPython.'''

    # No imports here.
    # print 'leo.py:getFileName',sys.argv

    i = 1
    while i &lt; len(sys.argv) and sys.argv[i].endswith('.py'):
        i += 1

