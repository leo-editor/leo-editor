    The subprocess will still get the ctrl + c signal.
    
    win32 implementation
    """
    old_handler = signal.signal(signal.SIGINT, signal.SIG_IGN)
    status = os.system(cmd)
    signal.signal(signal.SIGINT, old_handler)
