    #print 'pinfo par: &lt;%s&gt;' % parameter_s  # dbg

    # detail_level: 0 -&gt; obj? , 1 -&gt; obj??
    detail_level = 0
    # We need to detect if we got called as 'pinfo pinfo foo', which can
