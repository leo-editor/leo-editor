        if type(elem)==type([]):
            self.depth += 1
            self.__call__(elem,itpl('$pos$idx,'),**kw)
            self.depth -= 1
        else:
            printpl(kw['indent']*self.depth+'$pos$idx$kw["sep"]$elem')

