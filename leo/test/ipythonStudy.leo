def bzr_completer(self,event):
    """ Completer for bazaar commands """
    cmd_param = event.line.split()
    if event.line.endswith(' '):
        cmd_param.append('')

    if len(cmd_param) &gt; 2:
        cmd = cmd_param[1]
        param = cmd_param[-1]
