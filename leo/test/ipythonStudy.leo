<t tx="ekr.20071201085751.1161">def make_tab(lists):
    if hasattr(lists,"tolist"):
        lists=lists.tolist()
    ut=[]
    for rad in lists:
        if type(rad) in [list,tuple]:
            ut.append("\t".join(["%s"%x for x in rad]))
        else:
            ut.append("%s"%rad)
    return "\n".join(ut)            
    
