<t tx="ekr.20071201085751.1093">def make_list_of_list(txt):
    def make_num(x):
        try:
            return int(x)
        except ValueError:
            try:
                return float(x)
            except ValueError:
                try:
                    return complex(x)
                except ValueError:
                    return x
        return x                
    ut=[]
    flag=False
    for rad in [x for x in txt.split("\r\n") if x!=""]:
        raden=[make_num(x) for x in rad.split("\t")]
        if str in map(type,raden):
            flag=True
        ut.append(raden)
    return ut,flag


