         "allow_ctrl_c":allow_ctrl_c,
         "ctrl_c_tap_time_interval":ctrl_c_tap_time_interval,
         }
    if os.path.isfile(inputrcpath): 
        try:
            execfile(inputrcpath,loc,loc)
