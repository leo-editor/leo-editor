<t tx="ekr.20071201084507.42">def getkeypress(self):
    '''Return next key press event from the queue, ignoring others.'''
    while 1:
        e = self.get()
        if e.type == 'KeyPress' and e.keycode not in key_modifiers:
            log(e)
            if e.keysym == 'Next':
                self.scroll_window(12)
            elif e.keysym == 'Prior':
                self.scroll_window(-12)
            else:
                return e

