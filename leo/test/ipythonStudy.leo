<t tx="ekr.20071201085751.1057">def __delitem__(self,key):
    """ del db["key"] """
    fil = self.root / key
    self.cache.pop(fil,None)
    try:
        fil.remove()
    except OSError:
        # notfound and permission denied are ok - we
        # lost, the other process wins the conflict
        pass
    
