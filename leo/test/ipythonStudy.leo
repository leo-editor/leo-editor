                    # we have to mirror bits
                    attr = (attr &amp; ~0x70) | ((part &amp; 0x1) &lt;&lt; 6) | ((part &amp; 0x2) &lt;&lt; 4) | ((part &amp; 0x4) &lt;&lt; 2)
