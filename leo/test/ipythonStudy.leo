<t tx="ekr.20071201085751.1028">def read_md5(self):
    """ Calculate the md5 hash for this file.

    This reads through the entire file.
    """
    f = self.open('rb')
    try:
        m = md5.new()
        while True:
            d = f.read(8192)
            if not d:
                break
            m.update(d)
    finally:
        f.close()
    return m.digest()

