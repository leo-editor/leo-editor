<t tx="ekr.20130807203905.16447">def dump(self,aDict):
    if not aDict: return '&lt;None&gt;'
    result = []
    for key in aDict:
        result.append('')
        val = aDict.get(key)
        result.append('%s: %s' % (key,pp.pformat(val)))
    return '\n'.join(result)

