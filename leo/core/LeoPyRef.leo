<t tx="EKR.20040517080250.28">def write_path(self, node, f):
    result = []
    while node:
        result.append(node.h)
        node = node.parent()
    result.reverse()
    if result:
        result2 = result[: -1]
        if result2:
            result2 = ' / '.join(result2)
            f.write("&lt;p&gt;\n")
            f.write("&lt;br /&gt;\n")
            f.write(escape(result2))
            f.write("&lt;br /&gt;\n")
            f.write("&lt;/p&gt;\n")
        f.write("&lt;h2&gt;")
        f.write(escape(result[-1]))
        f.write("&lt;/h2&gt;\n")
</t>
<t tx="EKR.20040517080250.30">def split_leo_path(self, path):
    '''Split self.path.'''
    if path == '/':
        return '/'
    if path.startswith("/"):
        path = path[1:]
    return path.split('/')
