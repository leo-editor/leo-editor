<t tx="ekr.20031218072017.3779">def updateOutlineMenu (self):

    c = self.c ; frame = c.frame
    if not c: return

    p = c.p
    hasParent = p.hasParent()
    hasBack = p.hasBack()
    hasNext = p.hasNext()
    hasChildren = p.hasChildren()
    isExpanded = p.isExpanded()
    isCloned = p.isCloned()
    isMarked = p.isMarked()

    try:
