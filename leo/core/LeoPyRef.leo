def do_verbatim(self):
    """
    Handle one token in verbatim mode.
    End verbatim mode when the appropriate comment is seen.
    """
    kind = self.kind
    #
    # Careful: tokens may contain '\r'
    val = regularize_nls(self.val)
    if kind == 'comment':
        if self.beautify_pat.match(val):
