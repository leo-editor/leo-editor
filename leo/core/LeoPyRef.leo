    """
    Create a Leonine version of the diffs that would be
    produced by a pull request between two branches.
    """
    if not directory:
        directory = os.path.join(g.app.loadDir, '..', '..')
    aList = g.execGitCommand(f"git rev-parse devel", directory)
    if aList:
