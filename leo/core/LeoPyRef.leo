        self.nodeItem[i].do_update()

        if blc:
            for link in blc.linksFrom(i):
                self.addLinkItem(i, link)
            for link in blc.linksTo(i):
                self.addLinkItem(link, i)

    for i in self.hierarchyLinkItem:
