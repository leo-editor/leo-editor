<t tx="ekr.20080416161551.206">def moveToNthChild(self, n: int):
    p = self
    if p.v and len(p.v.children) &gt; n:
        p.stack.append((p.v, p._childIndex),)
        p.v = p.v.children[n]
        p._childIndex = n
    else:
