<t tx="tbrown.20100226095909.12788">def get_line(self, c=None):
    """Get the next line of output from the last command"""

    if c and c != self.c:
        return None
    if not c:
        c = self.c
    if not self.output:
        self._get_output()
    if not self.output:
        g.es('No output retreived')
        return ''
    line = self.output[self.next_unread_line]
    self.next_unread_line -= 1
    return line
