<t tx="ekr.20210206062654.1">def get_sign_on(self, package):
    """Synchronous version of _sign_on"""
    g.app.computeSignon()
    signon = []
    for z in (g.app.signon, g.app.signon1):
        for z2 in z.split('\n'):
            signon.append(z2.strip())
    return self._make_response({"sign-on": "\n".join(signon)})
