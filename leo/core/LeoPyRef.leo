                    elif font_size &gt; 10:
                        if font_size &gt; 15:
                            text = '#{0}'.format(text)
                        elif font_size &gt; 12:
                            text = '##{0}'.format(text)
                        else:
                            text = '###{0}'.format(text)
                    elif char_format.fontFixedPitch(): #or format.fontFamily=='courier':
                        text = "`%1`".arg(text)
