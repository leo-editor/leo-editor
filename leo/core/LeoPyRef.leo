<t tx="ekr.20100803231223.5815">def isSectionLine(self,s,i):

    i = g.skip_ws(s,i)
    if not g.match(s,i,'['):
        return False,None,i
    k = s.find('\n',i+1)
    if k == -1: k = len(s)
    j = s.find(']',i+1)
    if -1 &lt; j &lt; k:
        return True,s[i:j+1],i
    else:
        return False,None,i
