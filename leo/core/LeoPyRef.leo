<t tx="ekr.20031218072017.3637">def compare_lines (self,s1,s2):

    if self.ignoreLeadingWhitespace:
        s1 = s1.lstrip()
        s2 = s2.lstrip()

    if self.ignoreInteriorWhitespace:
        k1 = g.skip_ws(s1,0)
        k2 = g.skip_ws(s2,0)
        ws1 = s1[:k1]
        ws2 = s2[:k2]
        tail1 = s1[k1:]
        tail2 = s2[k2:]
        tail1 = tail1.replace(" ","").replace("\t","")
        tail2 = tail2.replace(" ","").replace("\t","")
        s1 = ws1 + tail1
        s2 = ws2 + tail2

    return s1 == s2
