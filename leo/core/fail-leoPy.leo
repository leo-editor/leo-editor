def copyTreeFromSelfTo(self, p2: Position, copyGnxs: bool = False) -&gt; None:
    p = self
    p2.v._headString = g.toUnicode(p.h, reportErrors=True)  # 2017/01/24
    p2.v._bodyString = g.toUnicode(p.b, reportErrors=True)  # 2017/01/24
