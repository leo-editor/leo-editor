<t tx="ekr.20220401034726.3"># MatchAs(pattern? pattern, identifier? name)

def do_MatchAs(self, node: Node) -&gt; None:
    pattern = getattr(node, 'pattern', None)
    name = getattr(node, 'name', None)
    if pattern and name:
        self.visit(pattern)
        self.name('as')
        self.name(name)
    elif pattern:
        self.visit(pattern)  # pragma: no cover
    else:
        self.name(name or '_')
