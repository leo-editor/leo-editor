<t tx="ekr.20170806094319.8">def cleanAtCleanNode(self, p: Position) -&gt; bool:
    """Adjust whitespace in p, part of an @clean tree."""
    s = p.b.strip()
    if not s or p.h.strip().startswith('&lt;&lt;'):
        return False
    ws = '\n\n' if g.match_word(s, 0, 'class') else '\n'
    s2 = ws + s + ws
    changed = s2 != p.b
    if changed:
        p.b = s2
        p.setDirty()
    return changed
