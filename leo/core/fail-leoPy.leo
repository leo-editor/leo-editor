def do_match_case(self, node: Node) -&gt; None:

    guard = getattr(node, 'guard', None)
    body = getattr(node, 'body', [])
    self.name('case')
    self.visit(node.pattern)
    if guard:
        self.name('if')
        self.visit(guard)
    self.op(':')
    for statement in body:
