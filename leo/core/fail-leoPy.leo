        '    pass\n'
        '\n'
        "if __name__ == '__main__':\n"
        '    main()\n'
    )
    expected_results = (
        (0, '', # check_outline ignores the first headline
                '@others\n'
                "if __name__ == '__main__':\n"
                '    main()\n'
                '@language python\n'
                '@tabwidth -4\n'
