            if hasattr(z, 'arg') and z.arg is None:
                kwarg_arg = z
                keywords.remove(z)
                break
        # Sync the plain arguments.
        for z in args:
            self.arg_helper(z)
        # Sync the keyword args.
        for z in keywords:
            self.arg_helper(z.arg)
            self.op('=')
            self.arg_helper(z.value)
        # Sync the * arg.
        if star_arg:
            self.arg_helper(star_arg)
        # Sync the ** kwarg.
        if kwarg_arg:
            self.op('**')
            self.visit(kwarg_arg.value)
