# Full syntax diagram: # https://peps.python.org/pep-0634/#appendix-a

def do_Match(self, node: Node) -&gt; None:

    cases = getattr(node, 'cases', [])
    self.name('match')
    self.visit(node.subject)
    self.op(':')
    for case in cases:
