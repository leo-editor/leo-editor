<t tx="ekr.20230713150109.1">def undoParseBody(self) -&gt; None:
    """Restore p.b and delete all children."""
    u = self
    c = u.c
    p = u.p
    w = c.frame.body.wrapper
    p.deleteAllChildren()
    c.selectPosition(p)
    p.setDirty()
    p.b = u.oldBody
    w.setAllText(u.oldBody)
    w.setSelectionRange(0, 0, insert=0)
    w.setYScrollPosition(0)
