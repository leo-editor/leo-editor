                elif isinstance(z.value, (ast.List, ast.Tuple)):  # *[...]
                    # star_list = z
                    break
                raise AttributeError(f"Invalid * expression: {ast.dump(z)}")  # pragma: no cover
        # Scan keywords for **name.
