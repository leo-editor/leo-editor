<t tx="ekr.20080121105837.1418">def genScannableFiles(self, path=None, skipRareImports=False,
                      importableOnly=False):
    if path is None:
        path = self._getPath()
    searchedDirs = {}
    for dirname in path:
        #XXX Use os.walk().
        files = []
        os.path.walk(dirname, self._findScannableFiles,
                     (files, searchedDirs, skipRareImports,
                      importableOnly))
        for file in files:
            yield file

