        xpt.T_WCHAR             : "String",
        xpt.T_VOID              : "void",
        xpt.T_IID               : None,
        xpt.T_DOMSTRING         : "DOMString",
        xpt.T_CHAR_STR          : "String",
        xpt.T_WCHAR_STR         : "String",
