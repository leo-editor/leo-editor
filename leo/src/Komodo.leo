<t tx="ekr.20080121105837.1367">def _skipPastParenArguments(self, styles, text, p):
    paren_count = 1
    while p &lt; len(styles):
        if styles[p] == self.PHP_OPERATOR:
            if text[p] == "(":
                paren_count += 1
            elif text[p] == ")":
                if paren_count == 1:
                    return p+1
                paren_count -= 1
        p += 1
    return p

