<t tx="ekr.20080416071842.9">def main ():

    doLog()

    mgr = Manager(on_scan_complete=scanComplete)
    mgr.upgrade() # Can use mgr.db.upgrade methods for finer control.
    mgr.initialize()

    try:
        scan(mgr)
    finally:
        mgr.finalize()
        print 'done'

