<t tx="ekr.20080121105837.1400">def _gen_python_import_paths_from_pth_path(self, pth_path):
    pth_dir = dirname(pth_path)
    for line in open(pth_path, 'r'):
        line = line.strip()
        if line.startswith("#"): # comment line
            continue
        path = join(pth_dir, line)
        if exists(path):
            yield path

