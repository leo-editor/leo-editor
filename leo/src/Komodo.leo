            else:
                cplns = self.CSS_HTML_TAG_NAMES
            if DEBUG:
                print "  _async_eval_at_trg:: cplns:", cplns
            if cplns:
                ctlr.set_cplns( [ ("element", v) for v in cplns ] )
            ctlr.done("success")
        elif trg.id == ("CSS", TRG_FORM_CPLN, "anchors"):
            # Can be a colour or an id tag, depending upon what the
