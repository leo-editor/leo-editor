def lr0_goto_setnumber(I_setnumber, x):
    global Canonical
    global GotoSetNum

    if GotoSetNum.has_key((I_setnumber, x)):
        setnumber = GotoSetNum[(I_setnumber, x)]
    else:
        gset = lr0_goto(Canonical[I_setnumber], x)
        if not gset:
