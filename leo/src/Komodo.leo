<t tx="ekr.20080121105837.717">def _finish_list(self, collectionA, var_name, start_line, orig_tok, end_op, class_name):
    skipped_toks = [orig_tok] + self.skip_to_op(end_op)
    final_tok = self.tokenizer.get_next_token()
