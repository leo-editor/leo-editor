def get_variant(self):
    if not self.clock_hi_res &amp; 0x80:
        return RESERVED_NCS
    elif not self.clock_hi_res &amp; 0x40:
        return RFC_4122
    elif not self.clock_hi_res &amp; 0x20:
        return RESERVED_MICROSOFT
    else:
        return RESERVED_FUTURE

