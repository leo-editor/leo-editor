        start, end = token["start_index"], token["end_index"]
        if pos &lt; token["start_index"]:
            upper = idx
        elif pos &gt; token["end_index"]:
            lower = idx + 1
        else:
            return token
        sentinel -= 1
    else:
        raise Error("style_at_pos binary search sentinel hit: there "
                    "is likely a logic problem here!")

