def OrdPunctLast(value):
    result = []
    value = value.upper()
    for ch in value:
        i = ord(ch)
        if i &gt;= 0x21 and i &lt;= 0x2F:  # ch &gt;= '!' &amp;&amp; ch &lt;= '/'
            result.append(chr(i - ord("!") + ord('[')))    # ch - '!' + '['
        elif i &gt;= 0x3A and i &lt;= 0x40:  # ch &gt;= ':' &amp;&amp; ch &lt;= '@'
            result.append(chr(i - ord(":") + ord('[')))    # ch - ':' + '['
        else:
            result.append(ch)
    return "".join(result)

