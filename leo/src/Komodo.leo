                                    action[st,a] = j
                                    actionp[st,a] = p
                    
            except StandardError,e:
                raise YaccError, "Hosed in lalr_parse_table", e

        # Print the actions associated with each terminal
        if yaccdebug:
          for a,p,m in actlist:
            if action.has_key((st,a)):
                if p is actionp[st,a]:
                    _vf.write("    %-15s %s\n" % (a,m))
