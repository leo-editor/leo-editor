<t tx="ekr.20080121135406.157">def _favor_upto_color(self, cmds):
    # Favor the upto-color, but use the include-color if that's all they gave
    if cmds is None: return None
    color = None
    for cmd in cmds:
        if not isinstance(cmd, (list, tuple)):
            continue
        if cmd[0] == 'paint':
            if cmd[1]['type'] == 'upto':
                return cmd[1]['value']
            else:
                color = cmd[1]['value']
    return color


