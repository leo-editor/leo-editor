                #        lang_from_udl_family = {   # A plain XML can just contain
                #           'M': 'XML',             #   XML
                #        }
                lang_from_udl_family = %(lang_from_udl_family)r
            """ % locals()).lstrip()
    
        if not dry_run and not exists(dirname(lang_svc_path)):
