<t tx="ekr.20080121105837.1401">def _extra_dirs_from_env(self, env):
    extra_dirs = set()
    for pref in env.get_all_prefs("pythonExtraPaths"):
        if not pref: continue
        extra_dirs.update(d.strip() for d in pref.split(os.pathsep)
