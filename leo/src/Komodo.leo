    attrName = accessor.text_range(*accessor.contiguous_style_range_from_pos(trg.pos-3))
    if not attrName:
        log.warn("no attribute name in _StartSuggestAutoComplete")
        return []

    values = getValidAttributeValues(accessor, trg.pos, attrName, buf, trg)
