<t tx="swot.20210619083108.1">QScrollBar::horizontal {
   height: 8px;
}
QScrollBar::vertical {
   width: 8px;
}
QScrollBar::add-page, QScrollBar::sub-page {
   background: @gold-bg1;
}
QScrollBar::handle {
  background: @status-bg;
}
QScrollBar::handle:hover {
    background: @gold-focus;
}
QScrollBar::add-line, QScrollBar::sub-line {
   background: @gold-bg1;
}</t>
